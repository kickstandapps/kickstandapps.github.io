---
layout: nil
---
<?xml version="1.0" encoding="UTF-8"?>
<feed xmlns="http://www.w3.org/2005/Atom" xml:lang="en">
	<title type="text">{{ site.title }}</title>
	<subtitle type="html">{{ site.tagline }}</subtitle>
	<generator uri="https://github.com/mojombo/jekyll">Jekyll</generator>
	<icon>{{ site.baseurl }}/images/icon144.png</icon>
	<link rel="alternate" type="text/html" href="{{ site.baseurl }}" />
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>{{ site.baseurl }}/</id>
	<author>
		<name>{{ site.owner.name }}</name>
		<uri>{{ site.baseurl }}/</uri>
		<email>{{ site.owner.email }}</email>
	</author>
	<rights>Unless otherwise noted, all content is owned by Kickstand Apps.</rights>
	<atom10:link xmlns:atom10="http://www.w3.org/2005/Atom" rel="self" type="application/atom+xml" href="{{ site.baseurl }}/feex.xml" />
	{% for post in site.posts limit:50 %}
	<entry>
		<title type="html"><![CDATA[{{ post.title | cdata_escape }}]]></title>
		<link rel="alternate" type="text/html" href="{{ site.baseurl }}{{ post.url }}" />
		<id>{{ site.baseurl }}{{ post.id }}</id>
		{% if post.modified %}<updated>{{ post.modified | to_xmlschema }}T00:00:00-00:00</updated>
		<published>{{ post.date | date_to_xmlschema }}</published>
		{% else %}<published>{{ post.date | date_to_xmlschema }}</published>
		<updated>{{ post.date | date_to_xmlschema }}</updated>{% endif %}
		<author>
			<name>{{ site.owner.name }}</name>
			<uri>{{ site.baseurl }}</uri>
			<email>{{ site.owner.email }}</email>
		</author>
		<content type="html">{% if post.image.feature %}&lt;img src=&quot;{{ site.baseurl }}/images/{{ post.image.feature }}&quot; alt=&quot;{{ post.title }} page image&quot;&gt;&lt;br/&gt;{% endif %}{{ post.content | xml_escape }}
		&lt;p&gt;&lt;a href=&quot;{{ site.baseurl }}{{ post.url }}&quot;&gt;{{ post.title }}&lt;/a&gt; was originally published by &lt;a href=&quot;{{ site.baseurl }}/about.html&quot;&gt;{{ site.owner.name }}&lt;/a&gt; at &lt;a href=&quot;{{ site.baseurl }}&quot;&gt;{{ site.title }}&lt;/a&gt; on {{ post.date | date: "%B %d, %Y" }}.&lt;/p&gt;</content>
	</entry>
	{% endfor %}
</feed>
